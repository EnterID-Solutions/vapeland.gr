<!-- Start AlphaBank module -->
<form method="post" action="{{ action }}" id="alphabank-form" accept-charset="UTF-8" enctype="application/x-www-form-urlencoded">
 {% for field, val in formFields %}
    {% if val|length > 0 %}
        <input type="hidden" name="{{ field }}" value="{{ val }}" />
    {% endif %}
{% endfor %}
</form>
